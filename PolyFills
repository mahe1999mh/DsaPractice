

//map
Array.prototype.myMap = function(cb){

     let arr = [];
     for(let i=0; i<this.length; i++){
         arr.push(cb(this[i],i,this))
     }
     return arr;
}

//filter
Array.prototype.myFilter = function(callBack){
let newArr = [];
for(let i=0; i<this.length; i++){
   if(callBack(this[i],i,this)){
    newArr.push(this[i])
   }
}
return newArr
}





